<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC "-//Puppy Crawl//DTD Suppressions 1.2//EN"
  "https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
  <!-- Suppress the IllegalToken 'void' check only for test sources -->
  <!-- Use a file regex that matches Java files under src/test/java -->
  <suppress files=".*src/test/java/.*\.java$" checks="IllegalToken" message=".*LITERAL_VOID.*"/>
</suppressions>
